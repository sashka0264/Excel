{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended", 
    "google"
  ],
  "rules": {
    "linebreak-style": 0,
    "one-var": 0,
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "comma-dangle": ["error", "never"],
    "require-jsdoc": "off"
  }
}